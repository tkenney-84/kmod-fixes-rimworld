      <drawSize>(3, 2)</drawSize>
      <shadowData>
        <volume>(2.55, 0.75, 1.7)</volume>
      </shadowData>
    </graphicData>
    <descriptionHyperlinks>
      <ThingDef>GeneBank</ThingDef>
      <ThingDef>GeneProcessor</ThingDef>
      <ThingDef>Genepack</ThingDef>
      <ThingDef>Xenogerm</ThingDef>
